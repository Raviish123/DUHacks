<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.DUHacksPool" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#284e4f</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">#eddabf</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#023948</item>
        <item name="colorSecondaryVariant">#023948</item>
        <item name="colorOnSecondary">#023948</item>
        <item name="android:windowBackground">#eddabf</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.DUHacksPool.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.DUHacksPool.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.DUHacksPool.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>